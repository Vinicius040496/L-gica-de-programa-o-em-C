using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Lógica de programação questão 22
{
    internal class Program
    {
     //Faça um algoritmo que leia 10 valores, e imprima quais desses valores estão no intervalo entre 10-30. E imprima quais não fazem parte do intervalo passado
        static void Main(string[] args)
        {
            double[] numero = new double[10];
            double res;

            Console.WriteLine("Digite primeiro valor:");
            numero[0] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite segundo valor:");
            numero[1] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite terceiro valor:");
            numero[2] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite quarto valor:");
            numero[3] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite quinto valor:");
            numero[4] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite sexto valor:");
            numero[5] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite setimo valor:");
            numero[6] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite oitavo valor:");
            numero[7] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite nono valor:");
            numero[8] = double.Parse(Console.ReadLine());
            Console.WriteLine("Digite decimo valor:");
            numero[9] = double.Parse(Console.ReadLine());

            res = numero[0] = numero[1] = numero[2] = numero[3] = numero[4] = numero[5] = numero[6] = numero[7] = numero[8] = numero[9] / 10;

            Console.WriteLine("A soma aritmetica de todos os valores é: {0} ", res);

        }
    }
}
