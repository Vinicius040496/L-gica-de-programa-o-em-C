using System;

namespace Lógica_programação_questão10
{
    internal class Program
    {
        static void Main(string[] args)
        //Faça um algoritmo que leia dois valores, e indique qual é maior

        {
            Console.WriteLine(" Escreva o primeiro valor: ");
            int Valor1 = 0;
            Console.ReadLine();
            Console.WriteLine(" Escreva o segundo valor: ");
            int Valor2 = 0;
            Console.ReadLine();

            if (Valor1 >= Valor2)
            {
                Console.WriteLine(" O primeiro valor é maior! ");
            }
            else
            {
                Console.WriteLine(" O segundo valor é maior! ");
            }
        }
    }
}
